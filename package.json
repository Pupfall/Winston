{
  "name": "winston",
  "version": "1.0.0",
  "description": "API and GPT Action for domain search and registration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:https": "PORT=8443 USE_HTTPS=true ts-node src/index.ts",
    "dev:vercel": "vercel dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:https": "PORT=8443 USE_HTTPS=true node dist/index.js",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:sandbox": "RUN_NAMECHEAP_SANDBOX=1 jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:dev": "prisma migrate dev --name init && prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "domain",
    "registration",
    "api",
    "dns"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.3.2",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "prisma": "^6.17.1",
    "prom-client": "^15.1.3",
    "serverless-http": "^3.2.0",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/punycode": "^2.1.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vercel/node": "^3.0.21",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vercel": "^33.0.1"
  }
}
